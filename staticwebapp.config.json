{

  "platform": {

    "apiRuntime": "node:18"

  },

  "navigationFallback": {

    "rewrite": "/index.html",

    "exclude": ["/_next/*", "/api/*", "/assets/*", "/*.{css,js,json,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot}"]

  },

  "routes": [

    {

      "route": "/procurement/edit-product",

      "allowedRoles": ["anonymous", "authenticated"]

    },

    {
      "route": "/procurement/edit-products",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=60, must-revalidate"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=3600"
      }
    }

  ],

  "globalHeaders": {

    "cache-control": "public, max-age=3600"

  },

  "mimeTypes": {

    ".json": "application/json",

    ".js": "text/javascript",

    ".css": "text/css"

  }

}
 